export material standard_motorcycle_285_copper_Mat(*)
  = adobe::mtl::standard(
    translucence : float(0),
    interiorColor : color(1.0, 1.0, 1.0),
    indexOfRefraction : float(1.0000),
    metallic : float(1.0),
    baseColor : adobe::util::color_texture( texture_2d("standard_motorcycle_285/standard_motorcycle_285_copper_baseColor.png", ::tex::gamma_srgb) ),
    roughness : adobe::util::float_texture( texture_2d("standard_motorcycle_285/standard_motorcycle_285_copper_roughness.png", ::tex::gamma_linear) ),
    density : float(0),
    glow : float(0.0),
    opacity : float(1.0),
    normal : adobe::util::normal_texture( texture_2d("standard_motorcycle_285/standard_motorcycle_285_copper_normal.png", ::tex::gamma_linear) ),
    height : float(0.5),
    heightScale : float(1.0)
    );

